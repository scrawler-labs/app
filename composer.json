{
    "name": "scrawler/app",
    "description": "scrawler framework main app container",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Scrawler\\": "src/",
            "Tests\\Controllers\\": "tests/Controllers/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "authors": [
        {
            "name": "ipranjal",
            "email": "pranjaltech10@gmail.com"
        }
    ],
    "require": {
        "scrawler/router": "^4.1",
        "php-di/php-di": "^7.0",
        "phlak/config": "^8.0",
        "scrawler/http": "^1.0",
        "vlucas/phpdotenv": "^5.6"
    },
    "require-dev": {
        "pestphp/pest": "^3.2"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
